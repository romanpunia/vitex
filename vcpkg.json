{
  "name": "vitex",
  "description": "Cross-platform application development framework",
  "version": "1.0.0",
  "default-features": [
    "compression",
    "openssl",
    "mongodb",
    "postgresql",
    "sqlite"
  ],
  "features": {
    "compression": {
      "description": "Support for zlib compression algorithms",
      "dependencies": [
        "zlib"
      ]
    },
    "openssl": {
      "description": "Support for SSL/TLS secure connections, certificates, cryptographic algorithms including hashing, encryption and randomizer",
      "dependencies": [
        "openssl"
      ]
    },
    "mongodb": {
      "description": "Support for MongoDB client operations",
      "dependencies": [
        "mongo-c-driver"
      ]
    },
    "postgresql": {
      "description": "Support for PostgreSQL client operations",
      "dependencies": [
        "libpq"
      ]
    },
    "sqlite": {
      "description": "Support for SQLite client operations",
      "dependencies": [
        "sqlite3"
      ]
    }
  },
  "builtin-baseline": "96d5fb3de135b86d7222c53f2352ca92827a156b"
}